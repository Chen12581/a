<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
 	<package name="struts-jsonnewuser" extends="twdpe-common-default">
 	    <global-allowed-methods>regex:.*</global-allowed-methods>
 	    
 		<action name="newuser_*" class="com.talkweb.twdpe.webcomponent.component.newUserMngAction" method="{1}">
			<param name="aliases">#{'page' : 'pagination.currPage','pageSize' : 'pagination.size'}</param>
			<result name="success" type="json">
				<param name="root">data</param>
				<param name="excludeNullProperties">true</param>
			</result>
		</action>
 	</package>
</struts>
