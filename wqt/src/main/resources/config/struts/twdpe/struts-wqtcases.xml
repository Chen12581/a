<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="struts-iprcases"   extends="twdpe-common-default">
	    <global-allowed-methods>regex:.*</global-allowed-methods>
	   
        <action name="wqtCasesList" class="com.wqt.cases.action.CasesAction" method="wqtCasesList">
			<result name="success" type="freemarker">   
				 /cases/cases_list.ftl
			 </result>
			 <interceptor-ref name="workerServiceInterceptor" />  <!-- 应用自定义拦截器 -->
		</action>
		
		<action name="wqtCasesInfo" class="com.wqt.cases.action.CasesAction" method="wqtCasesInfo">
			<result name="success" type="freemarker">   
				 /cases/cases.ftl
			 </result>
			 <result name="404" >   
				 /404.html
			 </result>
			 <interceptor-ref name="workerServiceInterceptor" />  <!-- 应用自定义拦截器 -->
		</action>
		
	</package>
</struts>
