    <bean id="newsNewsLabelDao" class="com.wqt.news.dal.NewsNewsLabelDao" />
	<bean id="newsNewsLabelService" class="com.wqt.news.service.impl.NewsNewsLabelServiceImpl" /> 
	
</beans>