<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true"
	>
	<!-- IP控制管理 -->
	<bean id="com.talkweb.twdpe.base.ipcontrol.IIpControl" class="com.talkweb.twdpe.base.ipcontrol.IpControlDbImpl">
		<property name="manager" ref="com.talkweb.twdpe.base.ipcontrol.dal.IpControlManager" />
	</bean>
	<bean id="com.talkweb.twdpe.base.ipcontrol.dal.IpControlManager" class="com.talkweb.twdpe.base.ipcontrol.dal.IpControlManager" >
		<property name="session" ref="session" />
	</bean>
	<bean id="com.talkweb.twdpe.base.ipcontrol.service.IpControlService" class="com.talkweb.twdpe.base.ipcontrol.service.IpControlService">
		<property name="iIpControl" ref="com.talkweb.twdpe.base.ipcontrol.IIpControl" />
	</bean>
</beans>