<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true">

	<bean id="logConfigDao" class="com.talkweb.twdpe.base.log.dal.impl.LogConfigDaoImpl" />

	<bean id="logConfigService"
		class="com.talkweb.twdpe.base.log.service.impl.LogConfigServiceImpl" />

	<bean id="logInfoDao" class="com.talkweb.twdpe.base.log.dal.impl.LogInfoDaoImpl">
		<property name="defaultTableCode" value="DP_LOG_0001" />
	</bean>

	<bean id="logService" class="com.talkweb.twdpe.base.log.service.impl.LogServiceImpl" />

</beans>