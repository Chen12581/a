<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <!-- 调度引擎 init-method="init" -->
    <!--   
    <bean id="startup.scheduler" class="com.talkweb.twdpe.base.tasks.TaskLoader" init-method="init"></bean>
   -->
	<bean id="component.parameters" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<!-- 过滤词文件路径 -->
				<entry key="words-config" value="config/component/words.properties" />
				<entry key="datamapSwitch" value="true" />
				<entry key="user.initial.password" value="123456" />
				<entry key="cacheName" value="config.cache.ehcache" />
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="pwdEncoder" class="com.talkweb.twdpe.base.org.security.Md5PwdEncoder"/>
</beans>
