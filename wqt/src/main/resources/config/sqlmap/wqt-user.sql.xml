        <!--用户收藏 -->
        <select id="selectUserCollectList" 
	        parameterType="com.wqt.user.pojo.UserCollect" 
	        resultType="com.wqt.user.pojo.UserCollect">
		     select 
             a.*
             from v_usercollection a
		     where 1=1
			<if test="null!= userid and ''!= userid">
			   and   a.userid=#{userid}
			</if>
			order by collecttime desc
	   </select>
	   
        <!--用户绑定银行卡 -->
        <select id="selectListUserBank" 
	        parameterType="com.wqt.user.pojo.WqtUserBank" 
	        resultType="com.wqt.user.pojo.WqtUserBank">
		     select 
             a.*,
             b.bankname
             from wqt_user_bank a,wqt_bank b
		     where a.bankid=b.id
			<if test="null!= id and ''!= id">
			    and  a.id=#{id}
			</if>
			<if test="null!= userid and ''!= userid">
			   and   a.userid=#{userid}
			</if>
			<if test="null!= bankid and ''!= bankid">
			   and   a.bankid=#{bankid}
			</if>
			<if test="null!= accountno and ''!= accountno">
			   and   a.accountno=#{accountno}
			</if>
			<if test="null!= state and ''!= state">
			   and   a.state=#{state}
			</if>
			order by createtime desc
	   </select>
        
